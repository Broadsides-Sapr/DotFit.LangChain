<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net4.6.2;netstandard2.0;net6.0;net7.0;net8.0</TargetFrameworks>
        <NoWarn>$(NoWarn);CA2100;CA1305</NoWarn>
    </PropertyGroup>

    <PropertyGroup Label="NuGet">
        <Description>Postgres for LangChain.</Description>
        <PackageTags>$(PackageTags);postgres</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.SemanticKernel.Connectors.Memory.Postgres" />
    </ItemGroup>

</Project>
